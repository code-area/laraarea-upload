{
	"name": "code-area/laraarea-upload",
	"description": "Upload for laravel",
	"keywords": ["laravel", "upload"],
	"license": "MIT",
	"authors": [
		{
			"name": "Codearea",
			"email": "davitzeynalyan@laravel-helper.com",
			"role": "Developer"
		}
	],
	"require": {
		"laravel/framework": "^5.4",
		"code-area/laraarea-validator": "1.0.*"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0"

	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraAreaUpload\\Providers\\AreaUploadServiceProvider"
			]
		}
	},
	"suggest": {
		"code-area/laraarea-helpers": "Useful functions"
	},
	"autoload": {
		"psr-4": {
			"LaraAreaUpload\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
